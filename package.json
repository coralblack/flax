{
  "name": "@coralblack/flax",
  "version": "0.1.12",
  "description": "API Req-Res Guard for React",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "browser": "dist/index.browser.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10",
    "npm": ">=7.20.3 <8"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parcel serve example/index.html --open",
    "prebuild": "rm -rf dist/* && npm install --save @swc/helpers",
    "postbuild": "mv .styles dist/styles",
    "build": "npm run build:css && npm run build:lib",
    "build:lib": "parcel build src/index.ts --no-cache",
    "build:css": "parcel build src/styles/index.ts --no-cache && mkdir -p .styles && mv dist/index.css* .styles && rm -rf dist",
    "lint": "gts check",
    "lint:fix": "gts fix"
  },
  "dependencies": {
    "@swc/helpers": "^0.3.9",
    "axios": "^0.27.2",
    "node-cache": "^5.1.2",
    "p-cancelable": "^3.0.0",
    "query-string": "^7.1.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.5.0",
    "@parcel/transformer-sass": "^2.5.0",
    "@parcel/transformer-typescript-types": "^2.5.0",
    "@types/node": "^14.14.16",
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.16",
    "@types/react-router-dom": "^5.3.3",
    "buffer": "^6.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.29.4",
    "events": "^3.3.0",
    "gts": "^3.1.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "parcel": "^2.0.0",
    "process": "^0.11.10",
    "react-router-dom": "^5.3.1",
    "sass": "^1.51.0",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coralblack/flax.git"
  },
  "author": "CORAL.BLACK",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coralblack/flax/issues"
  },
  "homepage": "https://github.com/coralblack/flax#readme",
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "gts fix",
      "git add"
    ]
  }
}
